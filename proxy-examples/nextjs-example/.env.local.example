# Get your API key from the PromptQL dashboard
PROMPTQL_API_KEY=your_api_key_here

# PromptQL API Endpoint (SSL)
PROMPTQL_API_ENDPOINT=promptql.ddn.pro.hasura.io:443

# PromptQL API Host (for Host header)
PROMPTQL_API_HOST=promptql.ddn.pro.hasura.io

# Get your Personal Access Token (PAT) or Service Account Token from the PromptQL Console
PROMPTQL_ACCESS_TOKEN=your_pat_or_service_account_token_here

# Your PromptQL Project ID
PROMPTQL_PROJECT_ID=your_project_id_here

# Token refresh URL
PROMPTQL_TOKEN_REFRESH_URL=https://auth.pro.hasura.io/ddn/promptql/token